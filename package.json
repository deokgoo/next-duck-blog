    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "esbuild": "0.20.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.6.1",
